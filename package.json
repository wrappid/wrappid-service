{
  "name": "@wrappid/service",
  "version": "0.0.1",
  "description": "This is a service middleware to support wrappid framework development.",
  "main": "src/server.js",
  "scripts": {
    "prepare": "husky install",
    "start": "nodemon src/server.js",
    "test": "jest",
    "code:lint": "eslint --ignore-path .gitignore .",
    "code:format": "npm run code:lint -- --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git:https://github.com/wrappid/service.git"
  },
  "keywords": [
    "wrappid",
    "nodejs",
    "middleware"
  ],
  "author": "wrappid",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1433.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.10.0",
    "node-cron": "^3.0.2",
    "node-device-detector": "^2.0.14",
    "nodemailer": "^6.9.4",
    "otp-generator": "^4.0.1",
    "pg": "^8.11.0",
    "redis": "^4.6.7",
    "redis-json": "^6.0.3",
    "sequelize": "^6.32.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "husky": "^8.0.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts}",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
